{
  "compilerOptions": {
    "allowJs": false,
    "jsx": "react",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "target": "es6",
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node"
  },
  "exclude": ["coverage", "__setup__", "__tests__", "**/templates/**/*"],
  "typeAcquisition": {
    "enable": true
  }
}
